// PL/0 子集示例：完整关系运算符测试
// 测试所有关系运算符：<, <=, >, >=, ==, <>

var x, y;

begin
    x = 5;
    y = 5;
    
    // 测试 >=
    if (x >= y)
        write(1);
    
    // 测试 ==
    if (x == y)
        write(2);
    
    // 测试 <>
    y = 3;
    if (x <> y)
        write(3);
    
    // 测试 <
    if (y < x)
        write(4);
    
    // 测试 >
    if (x > y)
        write(5);
    
    // 测试 <=
    if (y <= x)
        write(6);
end
